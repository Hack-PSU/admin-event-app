{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "routes/*": ["./src/routes/*"],
      "components/*": ["./src/components/*"],
      "assets": ["./assets"],
      "assets/*": ["./assets/*"],
      "types": ["./src/types"],
      "types/*": ["./src/types/*"]
    },
    "esModuleInterop": true
  }
}
